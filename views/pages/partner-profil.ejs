<!DOCTYPE html>
<html lang="en">
<head>

	<% include ../partials/head-parsley %>
	<% include ../partials/head-kendo %>
	<% include ../partials/head-common %>

	<title>Računi - Partner</title>

</head>
<body>

<% include ../partials/header %>

<div class="container">
	<div class="row">
		<div class="col-xs-12 col-sm-12 col-md-12">
			<h4>Izmena podataka o partneru</h4>
		</div>
	</div>
	<form id="partner-form" data-parsley-validate="">
		<div class="row">
			<div class="col-xs-12 col-sm-5 col-md-4">
				<label for="naziv">Naziv</label>
				<input id="naziv" name="naziv" value="<%= partner.naziv %>" type="text" class="form-control" data-parsley-trigger="blur" required="" />
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12 col-sm-10 col-md-9">
				<hr />
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12 col-sm-5 col-md-4">
				<div class="row">
					<div class="col-xs-12 col-sm-12 col-md-12">
						<label for="korisnik">Korisnik</label>
						<input id="korisnik" name="korisnik" value="<%= partner.korisnik %>" type="text" class="input-sm form-control" data-parsley-trigger="blur" required="" />
					</div>
				</div>
				<div class="row">
					<div class="col-xs-6 col-sm-6 col-md-6">
						<label for="dan_izdavanja">Dan izdavanja</label>
						<select id="dan_izdavanja" name="dan_izdavanja" type="text" class="input-sm form-control">
							<% for(var i=1; i < 32; i++) { %>
							<option value="<%= i %>" <%= i == partner.dan_izdavanja ? 'selected=selected' : '' %>><%= i %>. u mesecu</option>
							<% }; %>
						</select>
					</div>
					<div class="col-xs-6 col-sm-6 col-md-6">
						<label for="dan_valute">Dan valute</label>
						<select id="dan_valute" name="dan_valute" type="text" class="input-sm form-control">
							<% for(var i=1; i < 32; i++) { %>
							<option value="<%= i %>" <%= i == partner.dan_valute ? 'selected=selected' : '' %>><%= i %>. u mesecu</option>
							<% }; %>
						</select>
					</div>
				</div>
				<div class="row">
					<div class="col-xs-8 col-sm-8 col-md-8">
						<label for="popust_id">Popust</label>
						<select id="popust_id" name="popust_id" value="<%= partner.popust_id %>" type="text" class="input-sm form-control" data-parsley-trigger="blur" required="">
							<% popusti.forEach(function(popust) { %>
							<option value="<%= popust.id %>" <%= partner.popust_id == popust.id ? 'selected=selected' : '' %>><%= popust.tip_popusta %></option>
							<% }); %>
						</select>
					</div>
					<div class="col-xs-4 col-sm-4 col-md-4">
						<label for="popust">Iznos</label>
						<input id="popust" name="popust" value="<%= partner.popust %>" type="text" class="input-sm form-control" data-parsley-trigger="blur" required="" />
					</div>
				</div>
			</div>
			<div class="col-xs-12 col-sm-5 col-md-offset-1 col-md-4">
				<div class="row">
					<div class="col-xs-12 col-sm-12 col-md-12">
						<label for="tekuci_racun">Tekući račun partnera</label>
						<input id="tekuci_racun" name="tekuci_racun" value="<%= partner.tekuci_racun %>" type="text" class="input-sm form-control" data-parsley-trigger="blur" required="" />
					</div>
				</div>
				<div class="row">
					<div class="col-xs-4 col-sm-4 col-md-4">
						<label for="model">Model</label>
						<input id="model" name="model" value="<%= partner.model %>" type="text" class="input-sm form-control" />
					</div>
					<div class="col-xs-8 col-sm-8 col-md-8">
						<label for="poziv_na_broj">Poziv na broj</label>
						<input id="poziv_na_broj" name="poziv_na_broj" value="<%= partner.poziv_na_broj %>" type="text" class="input-sm form-control" />
					</div>
				</div>
			</div>
		</div>
	</form>
	<div class="row">
		<div class="col-xs-12 col-sm-10 col-md-9">
			<hr />
		</div>
	</div>
	<div class="row">
		<div class="col-xs-12 col-sm-10 col-md-9">
			<div class="pull-right">
				<button type="button" class="btn btn-success btn-sm pull-right" id="updatePartner">
					<span class="glyphicon glyphicon-floppy-save"></span>
					Sačuvaj
				</button>
				<button type="button" class="btn btn-danger btn-sm" style="margin-right: 10px;" data-toggle="modal" data-target="#confirmDeleteModal">
					<span class="glyphicon glyphicon-floppy-remove"></span>
					Izbriši
				</button>
			</div>
			<button type="button" class="btn btn-link btn-sm pull-left" onclick="location.href='/partneri'">
				<span class="glyphicon glyphicon-chevron-left"></span>
				Nazad
			</button>
		</div>
	</div>
</div>

<% include ../partials/footer %>

<% include ../partials/body-common %>
<% include ../partials/body-kendo %>
<% include ../partials/body-parsley %>
<script src="js/parner-profil.js"></script>

</body>
</html>
